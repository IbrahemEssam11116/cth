<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>CTH | @ViewData["Title"]</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/assets/images/favicon.ico">

    <!-- Bootstrap select pluings -->
    <link href="~/assets/libs/bootstrap-select/bootstrap-select.min.css" rel="stylesheet" type="text/css" />

    <!-- c3 plugin css -->
    <link rel="stylesheet" type="text/css" href="~/assets/libs/c3/c3.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/parimaryUpdate.css"/>
    <!-- App css -->
    <link href="~/assets/css/custome.css" rel="stylesheet" type="text/css" />

    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/app.min.css" rel="stylesheet" type="text/css" />
    @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">*@
    <link href="~/assets/css/sweetalert/sweetalert.css" rel="stylesheet" />
    <link href="~/assets/css/toastr/toastr.min.css" rel="stylesheet" />
    <!-- Vendor js -->
    <script src="~/assets/js/vendor.min.js"></script>
<style>
    .footer1{
    bottom: 0;
    padding: 21px 15px 20px;
    position: absolute;
    right: 0;
    color: #98a6ad;
    left: 0px;
    background-color: #eee;
    }
</style>
</head>

<body>
    <partial name="_Header" />
    @await Component.InvokeAsync("SStorm.CTH.Web.Notify")


    <!-- Begin page -->
    <div id="wrapper">

        



        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="container-fluid text-center">
            <!-- Start Content-->
            <div class="content">
                <br /><br /><br /><br />
                @RenderBody()


            </div>
            <!-- end content -->
            <!-- Footer Start -->
            <footer class="footer1 text-center">
                <partial name="_Footer" />
            </footer>
            <!-- end Footer -->

        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->

    </div>
    <!-- END wrapper -->
    <!-- Bootstrap select plugin -->
    <script src="~/assets/libs/bootstrap-select/bootstrap-select.min.js"></script>

    <!-- plugins -->
    <script src="~/assets/libs/c3/c3.min.js"></script>
    <script src="~/assets/libs/d3/d3.min.js"></script>

    <!-- dashboard init -->
    <script src="~/assets/js/pages/dashboard.init.js"></script>

    <!-- App js -->
    <script src="~/assets/js/app.min.js"></script>
    <script src="~/assets/js/UtilsScript.js"></script>
    <script src="~/assets/js/sweetalert/sweetalert.js"></script>
    <script src="~/assets/js/toastr/toastr.min.js"></script>
    @RenderSection("Scripts", required: false)



</body>
</html>